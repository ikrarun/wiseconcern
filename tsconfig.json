{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./src",
    "allowJs": true,
    "checkJs": true,
    "paths": {
      "@/*": [
        "./components/*"
      ],
      "#/*": [
        "./layouts/*"
      ],
      "$/*": [
        "./content/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}